@model IEnumerable<VehicleOverviewViewModel>;

@inject UserManager<ApplicationUser> UserManager

@{
    ViewData["Title"] = $"{UserManager.GetUserName(User)}'s garage";
}

<div class="row">
    <div class="col-md-12">
        <h1>@ViewBag.Title</h1>
        <h3>Welcome to your garage!</h3>
        <p>
            <a asp-controller="Garage" asp-action="AddVehicle" class="btn btn-default">Add a new Vehicle</a>
        </p>
        </div>
    </div>

<h4>Your vehicles</h4>
<partial name="../Home/_VehicleOverviewPartial" model="Model" />