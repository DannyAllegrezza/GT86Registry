@model IEnumerable<VehicleOverviewViewModel>;

@inject UserManager<ApplicationUser> UserManager

@{
    ViewData["Title"] = $"{UserManager.GetUserName(User)}'s Garage";
}

<div class="row">
    <div class="col-md-12">
        <h2>@ViewBag.Title</h2>

        @if (Model.Count() == 0)
        {
        
        <p>It looks like your garage is empty. To get started, <a asp-controller="Garage" asp-action="AddVehicle">add a new vehicle</a>
        </p>
        }
    </div>
</div>

<h4>Your vehicles</h4>
<partial name="../Home/_VehicleOverviewPartial" model="Model" />